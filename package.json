{
  "name": "sole-x",
  "version": "1.0.0",
  "description": "nft marketplace",
  "main": "src/app.ts",
  "scripts": {
    "build": "yarn tsc && yarn copy-files",
    "start": "nodemon --watch \"src/**/*.ts\" --ignore \"node_modules/**/*\" ",
    "start:local": "cross-env NODE_ENV=local nodemon --watch \"src/**/*.ts\" --ignore \"node_modules/**/*\"",
    "start:dev": "cross-env NODE_ENV=development nodemon --watch \"src/**/*.ts\" --ignore \"node_modules/**/*\"",
    "inspect": "nodemon --inspect src/app.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "yarn --exec ts-node  typeorm migration:generate -f src/config/ormconfig.ts -n",
    "migration:run": "yarn  typeorm migration:run -f src/config/ormconfig.ts ",
    "migration:revert": "yarn typeorm migration:revert -f src/config/ormconfig.ts ",
    "migration:show": "yarn typeorm migration:show -f src/config/ormconfig.ts",
    "migration:create": "yarn typeorm migration:create -f src/config/ormconfig.ts",
    "copy-files": "cp ./src/resources/swagger.json ./dist/resources/swagger.json"
  },
  "author": "ozys",
  "license": "ISC",
  "dependencies": {
    "argon2": "^0.28.2",
    "bignumber.js": "^9.0.1",
    "body-parser": "^1.19.0",
    "bull": "^3.22.9",
    "caver-js": "^1.5.0",
    "caver-js-ext-kas": "^1.0.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "dotenv-flow": "^3.2.0",
    "errorhandler": "^1.5.1",
    "eth-sig-util": "^3.0.1",
    "ethereumjs-util": "^7.0.10",
    "event-dispatch": "^0.4.1",
    "eventemitter3": "^4.0.7",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "express-jwt": "^6.0.0",
    "express-validator": "^6.12.0",
    "fs": "0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "mime": "^2.5.2",
    "moment": "^2.29.0",
    "moment-timezone": "^0.5.33",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "node-cron": "^2.0.3",
    "nodemailer": "^6.6.2",
    "passport": "^0.4.1",
    "passport-google-oauth": "^2.0.0",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "request-promise": "^4.2.6",
    "socket.io": "^3.0.5",
    "socket.io-emitter": "^3.2.0",
    "socket.io-redis": "^6.1.1",
    "ts-node": "^10.0.0",
    "tsc": "^2.0.3",
    "tsconfig-paths": "^3.9.0",
    "typedi": "^0.10.0",
    "typeorm": "^0.2.34",
    "web3": "^1.3.6",
    "web3-eth-abi": "^1.3.6",
    "winston": "^3.3.0",
    "winston-daily-rotate-file": "^4.5.5",
    "winston-slack-webhook-transport": "^2.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@types/lodash": "^4.14.170",
    "@types/node": "^15.12.5",
    "nodemon": "^2.0.7",
    "swagger-ui-express": "^4.1.6",
    "typescript": "^4.3.4"
  }
}
